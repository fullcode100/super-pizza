{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["airbnb", "airbnb/hooks", "eslint:recommended", "prettier", "prettier/react"],
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": ["react", "react-hooks", "jsx-a11y", "import", "prettier"],
	"rules": {
		"prettier/prettier": ["error"],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/jsx-uses-react": "off",
		"react/react-in-jsx-scope": "off",
		"no-console": "off",
		"no-debugger": "off",
		"@typescript-eslint/no-explicit-any": "off"
	}
}
